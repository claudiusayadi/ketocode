		"terminal.background": "#000000",
		"terminal.foreground": "#d0d0d0",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#ff5370",
		"terminal.ansiGreen": "#c3e88d",
		"terminal.ansiYellow": "#ffcb6b",
		"terminal.ansiBlue": "#82aaff",
		"terminal.ansiMagenta": "#c792ea",
		"terminal.ansiCyan": "#89ddff",
		"terminal.ansiWhite": "#ffffff",
		"terminal.ansiBrightBlack": "#545454",
		"terminal.ansiBrightRed": "#ff5370",
		"terminal.ansiBrightGreen": "#c3e88d",
		"terminal.ansiBrightYellow": "#ffcb6b",
		"terminal.ansiBrightBlue": "#82aaff",
		"terminal.ansiBrightMagenta": "#c792ea",
		"terminal.ansiBrightCyan": "#89ddff",
		"terminal.ansiBrightWhite": "#ffffff"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#676e95",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": ["string", "constant.character.escape"],
			"settings": {
				"foreground": "#c3e88d"
			}
		},
		{
			"name": "Numbers & Constants",
			"scope": ["constant.numeric", "constant.language", "constant.other"],
			"settings": {
				"foreground": "#f78c6c"
			}
		},
		{
			"name": "Keywords & Control Flow",
			"scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "Functions & Methods",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#82aaff"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "variable.other.readwrite", "meta.variable"],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Parameters",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#ff9cac"
			}
		},
		{
			"name": "Classes, Types & Interfaces",
			"scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Properties & Object Keys",
			"scope": ["variable.other.property", "meta.object-literal.key", "support.type.property-name"],
			"settings": {
				"foreground": "#b2ccd6"
			}
		},
		{
			"name": "Tags (HTML/XML)",
			"scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#89ddff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Decorators",
			"scope": ["meta.decorator", "entity.name.function.decorator"],
			"settings": {
				"foreground": "#82aaff"
			}
		},
		{
			"name": "Regex",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
			"settings": {
				"foreground": "#82aaff"
			}
		},
		{
			"name": "JSON Key - Level 1",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "JSON Key - Level 2",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#c3e88d"
			}
		},
		{
			"name": "JSON Key - Level 3",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "JSON Key - Level 4",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": ["markup.heading", "entity.name.section.markdown"],
			"settings": {
				"foreground": "#c792ea",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": ["markup.bold"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f07178"
			}
		},
		{
			"name": "Markdown Code",
			"scope": ["markup.inline.raw", "markup.raw.block"],
			"settings": {
				"foreground": "#c3e88d"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#c3e88d"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#ff5370"
			}
		}
	],
	"semanticTokenColors": {
		"class": "#ffcb6b",
		"interface": "#ffcb6b",
		"enum": "#ffcb6b",
		"namespace": "#ffcb6b",
		"type": "#ffcb6b",
		"typeParameter": "#ffcb6b",
		"parameter": "#ff9cac",
		"variable": "#f07178",
		"variable.readonly": "#f07178",
		"property": "#b2ccd6",
		"property.readonly": "#b2ccd6",
		"function": "#82aaff",
		"method": "#82aaff",
		"keyword": "#c792ea",
		"string": "#c3e88d",
		"number": "#f78c6c",
		"operator": "#89ddff",
		"comment": "#676e95"
	}
}
