		"descriptionForeground": "#808080",
		"errorForeground": "#ff5370",
		"widget.shadow": "#000000",
		"selection.background": "#202020",

		"textLink.foreground": "#82aaff",
		"textLink.activeForeground": "#c792ea",

		"button.background": "#007fd4",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#006ab1",

		"dropdown.background": "#050505",
		"dropdown.border": "#1a1a1a",
		"dropdown.foreground": "#d0d0d0",

		"input.background": "#050505",
		"input.border": "#1a1a1a",
		"input.foreground": "#d0d0d0",
		"input.placeholderForeground": "#505050",
		"inputOption.activeBorder": "#007fd4",

		"scrollbar.shadow": "#000000",
		"scrollbarSlider.activeBackground": "#007fd450",
		"scrollbarSlider.background": "#30303050",
		"scrollbarSlider.hoverBackground": "#50505050",

		"badge.background": "#007fd4",
		"badge.foreground": "#ffffff",

		"list.activeSelectionBackground": "#1a1a1a",
		"list.activeSelectionForeground": "#ffffff",
		"list.dropBackground": "#202020",
		"list.focusBackground": "#1a1a1a",
		"list.focusForeground": "#ffffff",
		"list.highlightForeground": "#007fd4",
		"list.hoverBackground": "#0d0d0d",
		"list.inactiveSelectionBackground": "#0d0d0d",

		"activityBar.background": "#000000",
		"activityBar.foreground": "#d0d0d0",
		"activityBar.activeBorder": "#007fd4",
		"activityBarBadge.background": "#007fd4",
		"activityBarBadge.foreground": "#ffffff",

		"sideBar.background": "#000000",
		"sideBar.foreground": "#a0a0a0",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.foreground": "#d0d0d0",
		"sideBarTitle.foreground": "#bbbbbb",

		"editorGroup.border": "#000000",
		"editor.background": "#080808",
		"editor.foreground": "#d0d0d0",
		"editorLineNumber.foreground": "#3a3a3a",
		"editorLineNumber.activeForeground": "#d0d0d0",
		"editorCursor.foreground": "#ffffff",
		"editor.selectionBackground": "#262626",
		"editor.inactiveSelectionBackground": "#1a1a1a",
		"editor.lineHighlightBackground": "#0d0d0d",
		"editorLink.activeForeground": "#82aaff",

		"minimap.background": "#000000",

		"tab.activeBackground": "#080808",
		"tab.activeForeground": "#d0d0d0",
		"tab.activeBorderTop": "#007fd4",
		"tab.inactiveBackground": "#000000",
		"tab.inactiveForeground": "#606060",
		"tab.border": "#000000",

		"diffEditor.insertedTextBackground": "#c3e88d15",
		"diffEditor.removedTextBackground": "#ff537015",

		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#d0d0d0",
		"titleBar.inactiveBackground": "#000000",
		"titleBar.inactiveForeground": "#505050",

		"statusBar.background": "#000000",
		"statusBar.foreground": "#d0d0d0",
		"statusBar.border": "#1a1a1a",
